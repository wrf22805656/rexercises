install.packages("devtools")
library(devtools)
install_github('lchiffon/REmap')
library(REmap)
city_vec <- c("Beijing","上海")
get_city_coord("Beijing")
get_city_coord("上海")
get_geo_position(city_vec)

remap(mapdata, title ="", subtitle = "", theme=get_theme("Dark"))
set.seed(125)
origin = rep("Being",4)
destination = c("Shanghai", "Guangzhou", "Dalian", "Zigong")
dat <- data.frame(origin, destination)
out = remap(dat, title = "Remap", subtitle="Theme:Dark")
plot(out)
